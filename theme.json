{
  "name": "DeepSeek Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editorCursor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#2d2d2d",
    "editor.selectionBackground": "#3a3d41",
    "editor.inactiveSelectionBackground": "#3a3d41",
    "editorIndentGuide.background": "#404040",
    "editorIndentGuide.activeBackground": "#707070",
    "editor.selectionHighlightBackground": "#3a3d41",
    "editor.wordHighlightBackground": "#3a3d41",
    "editor.wordHighlightStrongBackground": "#3a3d41",
    "editor.findMatchBackground": "#515c6a",
    "editor.findMatchHighlightBackground": "#515c6a",
    "editor.findRangeHighlightBackground": "#3a3d41",
    "editor.hoverHighlightBackground": "#3a3d41",
    "editor.lineHighlightBorder": "#3a3d41",
    "editorLink.activeForeground": "#4e94ce",
    "editor.rangeHighlightBackground": "#3a3d41",
    "editorWhitespace.foreground": "#404040",
    "editorWidget.background": "#252526",
    "editorWidget.border": "#454545",
    "editorWidget.foreground": "#d4d4d4",
    "editorError.foreground": "#f48771",
    "editorWarning.foreground": "#cca700",
    "editorInfo.foreground": "#75beff",
    "editorHint.foreground": "#eeeeee",
    "editorGutter.background": "#1e1e1e",
    "editorGutter.modifiedBackground": "#1e1e1e",
    "editorGutter.addedBackground": "#1e1e1e",
    "editorGutter.deletedBackground": "#1e1e1e"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    }
  ]
}